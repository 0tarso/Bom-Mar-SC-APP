{
  "expo": {
    "name": "Bom Mar SC",
    "slug": "bom-mar-sc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-new-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-new-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "permissions": [
      "LOCATION"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.tarso.bommarsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-new-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tarso.bommarsc"
    },
    "web": {
      "favicon": "./assets/splash-new-icon.png"
    }
  }
}